                echo 'Building'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('test') {
            steps {
                echo 'testing'
            }
        }
         stage('release') {
            steps {
                echo 'releasing'
            }
        }
    }
}
